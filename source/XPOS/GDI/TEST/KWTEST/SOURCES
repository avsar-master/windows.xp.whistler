MAJORCOMP=windows
MINORCOMP=test

TARGETNAME=kwtest
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=\xp\neptune\xpos\inc

SOURCES=kwtest.c \
        res.rc

UMTYPE=windows
UMAPPL=kwtest
UMLIBS=\xp\neptune\sdk\lib\*\user32.lib \
       \xp\neptune\sdk\lib\*\kernel32.lib \
       \xp\neptune\sdk\lib\*\gdi32.lib \
       obj\*\kwtest.lib \
       obj\*\res.res
