Copyright (c) 2000  Microsoft Corporation

Module Name:

    sources.

Abstract:

    This file specifies the target component being built and the list of
    sources files needed to build that component.  Also specifies optional
    compiler switches and libraries that are unique for the component being
    built.


Author:

    Steve Wood 8/2/91

NOTE:   Commented description of this file is in \xp\bak\bin\sources.tpl

!ENDIF


!IF 0
!IFDEF WINDBG
XPDEBUG=ntsd
XPDEBUGTYPE=both
!ENDIF

386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF


!IFDEF CVP
XPDEBUG=cvp
XPDEBUGTYPE=ntsd
386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF



!IFDEF CAP
XPDEBUG=
XPDEBUGTYPE=
386_OPTIMIZATION=
386_PERFFLAGS=-Gh
!ENDIF


MAJORCOMP=wins
MINORCOMP=com

TARGETNAME=com
TARGETPATH=obj
TARGETTYPE=LIBRARY

#TARGETLIBS=\

!IFNDEF USENETBT
USENETBT=1
!ENDIF

!IFNDEF SUPPORT612WINS 
SUPPORT612WINS=1
!ENDIF

!IFNDEF REG_N_QUERY_SEP 
REG_N_QUERY_SEP=1
!ENDIF

!IFNDEF INIT_TIME_PAUSE_TEST 
INIT_TIME_PAUSE_TEST=1
!ENDIF

!IFNDEF QFE
QFE=0
!ENDIF

!IFNDEF NEWNETBTIF
NEWNETBTIF=1
!ENDIF

!IFNDEF TEST_HEAP
TEST_HEAP=0
!ENDIF

UNICODE=1
NET_C_DEFINES=-DUNICODE

COMMON_DEFINES=-DINCL32= -DNT -DWIN32 -DRPC_NO_WINDOWS_H -DTEST_DATA -DUSENETBT=$(USENETBT) -DQFE=$(QFE) -DSUPPORT612WINS=$(SUPPORT612WINS) -DNEWNETBTIF=$(NEWNETBTIF) -DREG_N_QUERY_SEP=$(REG_N_QUERY_SEP) -DINIT_TIME_PAUSE_TEST=$(INIT_TIME_PAUSE_TEST) -DTEST_HEAP=$(TEST_HEAP)

!IFDEF WINS_INTERACTIVE
C_DEFINES=$(COMMON_DEFINES) -DWINS_INTERACTIVE 
!ELSE
C_DEFINES=$(COMMON_DEFINES) 
!ENDIF

PRIVINC=$(BASEDIR)\neptune\inc
SDKINC=$(BASEDIR)\neptune\sdk\inc
INCLUDES=..\msg;..\inc;$(PRIVINC);$(SDKINC)

SOURCES= \
    commapi.c    \
    comm.c \
    assoc.c
