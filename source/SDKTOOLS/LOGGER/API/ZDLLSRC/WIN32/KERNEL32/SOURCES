MAJORCOMP=logger32
MINORCOMP=zernel32

TARGETNAME=zernel32
TARGETPATH=..\..\..\bin\win32
TARGETTYPE=DYNLINK
TARGETLIBS=$(BASEDIR)\neptune\sdk\lib\*\kernel32.lib \
       $(BASEDIR)\neptune\sdk\lib\*\gdi32.lib \
       $(BASEDIR)\neptune\sdk\lib\*\user32.lib \
       ..\..\..\bin\win32\*\logger32.lib \
       ..\hooks\obj\*\hooks.lib

LINKLIBS=$(BASEDIR)\neptune\sdk\lib\*\kernel32.lib

INCLUDES=..\inc;..\..\..\logsrc;..\..\inc

DLLBASE=0x5100000

SOURCES= tatom.c    \
         tkernel3.c \
         tcomm.c    \
         tconsole.c \
         tdebug.c   \
         tfile.c    \
         tfileio.c  \
         tgmem.c    \
         tinifile.c \
         tinput.c   \
         tlmem.c    \
         tmapio.c   \
         tmodule.c  \
         tnmpipe.c  \
         tresrc.c   \
         tsecurit.c \
         tsync.c    \
         ttask.c    \
         tvmem.c    \
         thand.c \
         kernel32.rc

I386_SOURCES= i386\takernel.asm

MIPS_SOURCES= mips\takernel.s

ALPHA_SOURCES= alpha\takernel.s

C_DEFINES=-DWIN32

BINPLACE_FLAGS=-d dump

UMTYPE=windows
UMTEST=
UMLIBS=
