MAJORCOMP=sdktools
MINORCOMP=cntfs

TARGETNAME=cntfs
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=cntfs.rc

UMTYPE=console
UMAPPL=cntfs
UMLIBS=obj\*\cntfs.lib
UMRES=obj\*\cntfs.res \xp\neptune\sdk\lib\*\binmode.obj

NTTARGETFILE0=msg.h

