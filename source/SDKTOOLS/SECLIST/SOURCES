MAJORCOMP=utils
MINORCOMP=seclist

TARGETNAME=seclist
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

SOURCES=seclist.cxx
BLDCRT=1

INCLUDES=..\..\utils\ulib\inc;..\..\utils\ifsutil\inc;..\..\utils\ufat\inc;..\..\utils\uhpfs\inc;\xp\neptune\sdk\inc

!IFDEF XPDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DUNICODE=1
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DSTACK_TRACE -DUNICODE=1
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DUNICODE=1
!ENDIF
!ENDIF
!ELSE	 # XPDEBUG
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=0 -DUNICODE=1
!ENDIF	 # XPDEBUG

CXXFLAGS=+d
UMLIBS=..\..\utils\ulib\src\obj\*\ulib.lib          \
       ..\..\utils\ifsutil\src\obj\*\ifsutil.lib    \
       ..\..\utils\ufat\src\obj\*\ufat.lib          \
       ..\..\utils\uhpfs\src\obj\*\uhpfs.lib	    \
       $(BASEDIR)\neptune\sdk\lib\*\ntdll.lib

UMTYPE=console

UMAPPL=seclist
